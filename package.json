    "clsx": "latest",
    "console-browserify": "latest",
    "constants-browserify": "latest",
    "crypto-browserify": "latest",
    "detect-browser-language": "latest",
    "domain-browser": "latest",
    "events": "latest",
    "fs": "latest",
    "https-browserify": "latest",
    "minimal-components-react": "latest",
    "next": "latest",
    "next-pwa": "latest",
    "nprogress": "latest",
    "os-browserify": "latest",
    "path-browserify": "latest",
    "polished": "latest",
    "process": "latest",
    "punycode": "latest",
    "querystring-es3": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-google-login": "latest",
    "react-intersection-observer": "latest",
    "react-modal": "latest",
    "react-paginate": "latest",
    "react-reveal": "latest",
    "react-router-dom": "latest",
